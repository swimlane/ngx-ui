{
  "name": "ngx-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "bundle-report": "webpack-bundle-analyzer dist/ngx-ui/stats.json",
    "prestart": "npm run build:css",
    "start": "ng serve --port 4200",
    "start-prod": "ng serve --prod --port 4200",
    "build": "npm run build:lib && ng build --prod",
    "build:demo": "npm run build:lib && cross-env NODE_ENV=production ng build --prod --statsJson=true --base-href=\"/ngx-ui/\"",
    "build:lib": "ng build @swimlane/ngx-ui --prod && npm run build:css",
    "build:css": "node scripts/prep-global-styles.js && npm run copy-files",
    "copy-files": "cp README.md dist/swimlane/ngx-ui/README.md && cp LICENSE dist/swimlane/ngx-ui/LICENSE",
    "package": "npm run build:lib",
    "test": "run-s test:*",
    "test:lint": "ng lint",
    "test:unit": "ng test @swimlane/ngx-ui --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:prettier": "prettier --check  \"{src,projects}/**/*.{js,ts,scss,css,md,json}\"",
    "test:integration": "start-server-and-test start http-get://localhost:4200 cypress:run",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write \"{src,projects}/**/*.{js,ts,scss,css,md,json}\"",
    "fix:lint": "ng lint --fix",
    "watch:unit": "ng test @swimlane/ngx-ui --watch",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "predeploy": "npm run build:demo",
    "deploy": "angular-cli-ghpages --dir ./dist/ngx-ui --name=Swimlane --email=developer@swimlane.com",
    "prepublish:lib": "npm run package",
    "publish:lib": "npm publish ./dist/swimlane/ngx-ui",
    "prepack": "npm run package",
    "pack": "npm pack ./dist/swimlane/ngx-ui",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "~9.2.1",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@swimlane/ng2-file-upload": "1.1.0",
    "@types/json-schema": "^7.0.4",
    "ajv": "^6.5.5",
    "codemirror": "^5.49.2",
    "d3-color": "^1.2.3",
    "moment-timezone": "^0.5.28",
    "mousetrap": "^1.6.5",
    "ng-in-viewport": "^6.1.5",
    "normalize.css": "^8.0.0",
    "prismjs": "^1.21.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/language-service": "~11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@ngneat/spectator": "^5.13.3",
    "@open-wc/semantic-dom-diff": "^0.19.3",
    "@swimlane/prettier-config-swimlane": "^3.0.2",
    "@types/codemirror": "0.0.80",
    "@types/faker": "^4.1.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/moment-timezone": "^0.5.13",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.11.1",
    "@types/prismjs": "^1.16.1",
    "angular-cli-ghpages": "^0.6.2",
    "autoprefixer": "^9.3.1",
    "clean-css": "^4.2.3",
    "codelyzer": "^6.0.0",
    "core-js": "^2.6.11",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "cypress": "^7.1.0",
    "faker": "^5.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.22.2",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.3.0",
    "ng-packagr": "^11.0.3",
    "ngx-moment": "^3.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.36",
    "postcss-url": "^8.0.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "shallow-render": "^11.0.0",
    "start-server-and-test": "^1.10.11",
    "ts-node": "^8.8.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-security": "^1.16.0",
    "tslint-config-swimlane": "^5.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "zone.js": "~0.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"{src,projects}/**/*.{js,ts,scss,css,md,json}\""
    }
  }
}
