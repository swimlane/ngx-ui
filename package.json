{
  "name": "@swimlane/ngx-ui",
  "version": "16.1.3",
  "description": "ngx-ui is a Angular2+ component UI library.",
  "main": "release/index.js",
  "typings": "release/index.d.ts",
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "build": "npm run build:icons && npm run build:webpack",
    "build:webpack": "webpack --display-error-details",
    "build:icons": "node ./config/icons.js",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "release": "cross-env NODE_ENV=production npm run build",
    "version": "standard-version",
    "package": "npm-run-all -s clean:release package:aot build:package package:minify",
    "package:ts": "tsc --outDir release",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:minify": "uglifyjs release/index.js --source-map release/index.min.js.map --source-map-url release/index.js.map --compress --mangle --screw-ie8 --output release/index.min.js",
    "deploy": "node ./config/deploy.js",
    "tag": "release-it"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ngx-ui.git"
  },
  "author": "Austin McDaniel",
  "license": "MIT",
  "keywords": [
    "angular2",
    "angularjs",
    "angular4",
    "angular",
    "components",
    "directives"
  ],
  "bugs": {
    "url": "https://github.com/swimlane/ngx-ui/issues"
  },
  "homepage": "https://github.com/swimlane/ngx-ui#readme",
  "peerDependencies": {
    "rxjs": "^5.2.0",
    "@angular/core": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/http": "^4.0.3"
  },
  "devDependencies": {
    "@angular/animations": "^4.1.1",
    "@angular/common": "^4.1.1",
    "@angular/compiler": "^4.1.1",
    "@angular/compiler-cli": "^4.1.1",
    "@angular/core": "^4.1.1",
    "@angular/forms": "^4.1.1",
    "@angular/http": "^4.1.1",
    "@angular/platform-browser": "^4.1.1",
    "@angular/platform-browser-dynamic": "^4.1.1",
    "@swimlane/ngx-datatable": "^9.1.0",
    "@types/node": "^7.0.18",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.0.1",
    "awesome-typescript-loader": "^3.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "codelyzer": "^3.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "d3-color": "^1.0.3",
    "duplicate-package-checker-webpack-plugin": "^1.0.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.3",
    "sass-loader": "^4.1.1",
    "source-map-loader": "^0.2.0",
    "standard-version": "^4.0.0",
    "style-loader": "^0.17.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.3.2",
    "uglifyjs": "2.4.10",
    "url-loader": "^0.5.8",
    "webfonts-generator": "^0.4.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0",
    "zone.js": "^0.8.10"
  },
  "dependencies": {
    "@angular/flex-layout": "2.0.0-beta.8",
    "angular2-moment": "^1.3.3",
    "codemirror": "^5.25.0",
    "moment": "^2.17.1",
    "mousetrap": "^1.6.1",
    "ng2-file-upload": "^1.2.1",
    "normalize.css": "^5.0.0"
  }
}
