    "npm-run-all": "^4.1.5",
    "postcss": "8.4.16",
    "postcss-url": "10.1.3",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "shallow-render": "18.0.0",
    "start-server-and-test": "1.14.0",
    "ts-node": "10.9.1",
    "typescript": "5.8.3",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
    "zone.js": "~0.15.0"
  },
  "volta": {
    "node": "22.16.0",
    "yarn": "4.9.2"
  },
  "packageManager": "yarn@4.9.2"
}
