{
  "name": "@swimlane/ngx-ui",
  "version": "21.4.3",
  "description": "ngx-ui is a component UI library for Angular.",
  "main": "release/index.js",
  "typings": "release/index.d.ts",
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "clean:build": "rimraf build",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "build": "npm run build:webpack",
    "build:webpack": "webpack --display-error-details",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "build:css": "postcss --use autoprefixer dist/*.css -d dist/",
    "build:sass": "node ./config/sass.js",
    "copy:build": "cpx -v \"src/**/*\" \"build\"",
    "copy:styles": "cpx -v \"build/**/*.css\" \"release\"",
    "release": "cross-env NODE_ENV=production npm run build",
    "version": "chg release -y && git add -A CHANGELOG.md",
    "package":
      "npm-run-all -s clean:release copy:build build:sass package:replace-scss package:aot build:package copy:styles package:minify clean:build",
    "package:ts": "tsc --outDir release",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:minify":
      "uglifyjs release/index.js --source-map --compress --mangle --screw-ie8 --output release/index.min.js",
    "package:replace-scss": "node ./config/replace.js",
    "prettier": "prettier --write \"src/**/*.{js,ts}\"",
    "deploy": "node ./config/deploy.js",
    "tag": "release-it",
    "test": "npm run cypress:run",
    "ci": "start-server-and-test start http-get://localhost:9999 cypress:run",
    "cypress:run": "cypress run",
    "cypress:start": "start-server-and-test start http://localhost:9999 cypress:open",
    "cypress:open": "cypress open"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ngx-ui.git"
  },
  "author": "Swimlane",
  "contributors": ["Austin McDaniel", "Marjan Georgiev", "Jayson Harshbarger"],
  "license": "MIT",
  "keywords": ["angular2", "angularjs", "angular4", "angular5", "angular", "components", "directives"],
  "bugs": {
    "url": "https://github.com/swimlane/ngx-ui/issues"
  },
  "homepage": "https://github.com/swimlane/ngx-ui#readme",
  "peerDependencies": {
    "rxjs": "^6.0.0",
    "@angular/cdk": "6.x",
    "@angular/core": "6.x",
    "@angular/common": "6.x",
    "@angular/animations": "6.x",
    "@angular/forms": "6.x"
  },
  "devDependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@swimlane/ngx-datatable": "^12.0.0",
    "@swimlane/prettier-config-swimlane": "^0.1.1",
    "@types/node": "^8.0.24",
    "ajv": "^6.4.0",
    "ajv-keywords": "^3.2.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.3",
    "chg": "^0.3.3",
    "clean-webpack-plugin": "^0.1.16",
    "codelyzer": "^4.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "core-js": "^2.5.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.5",
    "cypress": "^3.0.3",
    "d3-color": "^1.0.3",
    "duplicate-package-checker-webpack-plugin": "^1.2.5",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "fs-extra": "^4.0.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.12.1",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.6.1",
    "rxjs": "^6.1.0",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.0",
    "start-server-and-test": "^1.1.4",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-swimlane": "^3.0.3",
    "tslint-loader": "^3.6.0",
    "typescript": "~2.7.2",
    "uglify-js": "^3.3.16",
    "url-loader": "^0.5.8",
    "webfonts-generator": "^0.4.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0",
    "zone.js": "^0.8.26"
  },
  "dependencies": {
    "@angular/flex-layout": "6.0.0-beta.15",
    "ngx-moment": "^2.0.0",
    "codemirror": "^5.36.0",
    "moment": "^2.21.0",
    "mousetrap": "^1.6.1",
    "ng2-file-upload": "^1.2.1",
    "normalize.css": "^5.0.0"
  }
}
