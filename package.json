{
  "name": "swui",
  "version": "2.0.0",
  "description": "Swimlane UI Style Guide",
  "main": "bundles/swui.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "ngm": "ngm",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "build": "run-s ngm build:icons build:colors build:css build:copy",
    "build:webpack": "webpack --display-error-details",
    "build:icons": "node ./config/icons.js",
    "build:colors": "node ./config/colors.js",
    "build:css": "run-s build:scss build:postcss",
    "build:scss": "node-sass src/styles.scss dist/swui/css/swui.css --include-path src/styles --source-map true",
    "build:postcss": "postcss -r --map file dist/swui/css/swui.css",
    "build:copy": "cpy '**/*.*' ../../dist/swui/assets --cwd=src/assets --parents",
    "release": "NODE_ENV=production npm run build",
    "package": "NODE_ENV=package npm run build",
    "deploy": "node ./config/deploy.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/swui.git"
  },
  "author": "Austin McDaniel",
  "license": "MIT",
  "keywords": [
    "angular2",
    "angularjs"
  ],
  "bugs": {
    "url": "https://github.com/swimlane/swui/issues"
  },
  "homepage": "https://github.com/swimlane/swui#readme",
  "peerDependencies": {
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26",
    "@angular/common": "^2.2.0",
    "@angular/compiler": "^2.2.0",
    "@angular/core": "^2.2.0",
    "@angular/forms": "^2.2.0",
    "@angular/platform-browser": "^2.2.0",
    "@angular/platform-browser-dynamic": "^2.2.0"
  },
  "devDependencies": {
    "@angular/common": "^2.2.0",
    "@angular/compiler": "^2.2.0",
    "@angular/compiler-cli": "^2.2.0",
    "@angular/core": "^2.2.0",
    "@angular/forms": "^2.2.0",
    "@angular/platform-browser": "^2.2.0",
    "@angular/platform-browser-dynamic": "^2.2.0",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "~3.0.2",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.46",
    "@types/webpack": "2.0.0",
    "angular2-data-table": "^1.0.0",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "2.2.4",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "cpy-cli": "1.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-coverage": "^0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.11.2",
    "npm-run-all": "3.1.1",
    "npm-submodules": "0.1.2",
    "postcss-cli": "2.6.0",
    "postcss-loader": "^0.13.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "raw-loader": "^0.5.1",
    "rxjs": "5.0.0-beta.12",
    "sass-color-json": "^0.4.0",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "tslint": "3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.9",
    "url-loader": "^0.5.7",
    "webfonts-generator": "^0.3.5",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.10",
    "webpack-merge": "^0.15.0",
    "webpack-notifier": "^1.4.0",
    "zone.js": "^0.6.26"
  },
  "dependencies": {
    "angular2-moment": "1.0.0-beta.6",
    "codemirror": "^5.18.2",
    "highlight.js": "^9.7.0",
    "moment": "2.15.2",
    "ng2-codemirror": "^1.0.1",
    "ng2-file-upload": "^1.1.4-2",
    "normalize.css": "^4.2.0"
  }
}
