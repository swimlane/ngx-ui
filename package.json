{
  "name": "@swimlane/ngx-ui",
  "version": "19.1.1",
  "description": "ngx-ui is a component UI library for Angular.",
  "main": "release/index.js",
  "typings": "release/index.d.ts",
  "scripts": {
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "clean:build": "rimraf build",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "build": "npm run build:icons && npm run build:webpack",
    "build:webpack": "webpack --display-error-details",
    "build:icons": "node ./config/icons.js",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "build:css": "postcss --use autoprefixer dist/*.css -d dist/",
    "build:sass": "node ./config/sass.js",
    "copy:build": "cpx -v \"src/**/*\" \"build\"",
    "copy:styles": "cpx -v \"build/**/*.css\" \"release\"",
    "release": "cross-env NODE_ENV=production npm run build",
    "package": "npm-run-all -s clean:release copy:build build:sass package:replace-scss package:aot build:package copy:styles package:minify clean:build",
    "package:ts": "tsc --outDir release",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:minify": "uglifyjs release/index.js --source-map --compress --mangle --screw-ie8 --output release/index.min.js",
    "package:replace-scss": "node ./config/replace.js",
    "deploy": "node ./config/deploy.js",
    "tag": "release-it",
    "cypress:start": "webpack-dev-server & cypress open",
    "cypress:open": "cypress open"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ngx-ui.git"
  },
  "authors": [
    "Austin McDaniel",
    "Marjan Georgiev",
    "Jayson Harshbarger"
  ],
  "license": "MIT",
  "keywords": [
    "angular2",
    "angularjs",
    "angular4",
    "angular5",
    "angular",
    "components",
    "directives"
  ],
  "bugs": {
    "url": "https://github.com/swimlane/ngx-ui/issues"
  },
  "homepage": "https://github.com/swimlane/ngx-ui#readme",
  "peerDependencies": {
    "rxjs": "5.x",
    "@angular/core": "5.x",
    "@angular/common": "5.x",
    "@angular/animations": "5.x",
    "@angular/forms": "5.x"
  },
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@swimlane/ngx-datatable": "^11.0.3",
    "@types/node": "^8.0.24",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.3",
    "clean-webpack-plugin": "^0.1.16",
    "codelyzer": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "cypress": "^1.0.1",
    "d3-color": "^1.0.3",
    "duplicate-package-checker-webpack-plugin": "^1.2.5",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "fs-extra": "^4.0.1",
    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.7.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.4.3",
    "tslint-config-swimlane": "^3.0.3",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.28",
    "url-loader": "^0.5.8",
    "webfonts-generator": "^0.4.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0",
    "zone.js": "^0.8.16"
  },
  "dependencies": {
    "@angular/flex-layout": "2.0.0-beta.10-4905443",
    "angular2-moment": "^1.7.0",
    "codemirror": "^5.26.0",
    "moment": "^2.17.1",
    "mousetrap": "^1.6.1",
    "ng2-file-upload": "^1.2.1",
    "normalize.css": "^5.0.0"
  }
}
